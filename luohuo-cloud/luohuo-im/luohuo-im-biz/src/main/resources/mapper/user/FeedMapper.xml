<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luohuo.flex.im.core.user.mapper.TargetMapper">

    <select id="getDetail" resultType="com.luohuo.flex.im.core.user.mapper.FeedMapper">
        SELECT content, uid, media_type, create_time, id
        FROM im_feed WHERE id = #{id}
    </select>
</mapper>
