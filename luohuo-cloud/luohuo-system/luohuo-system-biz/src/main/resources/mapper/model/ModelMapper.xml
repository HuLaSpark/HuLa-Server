<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luohuo.flex.mapper.ModelMapper">
        <select id="selectAllEnabledModels" resultType="com.luohuo.flex.res.ModelVO">
            SELECT id, model_key, model_name, model_url, description, version, status
            FROM sys_model WHERE status = 1 ORDER BY id DESC
        </select>
</mapper>
