            <version>${luohuo-project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.luohuo.flex</groupId>
            <artifactId>luohuo-generator-entity</artifactId>
            <version>${luohuo-project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.luohuo.flex</groupId>
            <artifactId>luohuo-oauth-api</artifactId>
            <version>${luohuo-project.version}</version>
        </dependency>

        <dependency>
            <groupId>com.luohuo.flex</groupId>
            <artifactId>luohuo-file-sdk</artifactId>
            <version>${luohuo-project.version}</version>
        </dependency>

        <dependency>
            <groupId>com.luohuo.flex</groupId>
            <artifactId>luohuo-data-scope-sdk</artifactId>
            <version>${luohuo-project.version}</version>
        </dependency>
        <dependency>
            <groupId>com.luohuo.basic</groupId>
